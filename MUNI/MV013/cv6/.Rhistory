#QQ plot
my_qqPlot_binom <- function(x, beta, n , p ){
x <- sort(x)
alpha <- (seq(1, n)-beta)/ (n + 1 -2*beta)
F_alpha <- qnorm(alpha)
plot(F_alpha,x)
}
